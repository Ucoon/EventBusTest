<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center">

    <TextView
        android:id="@+id/tv_event_info_priority"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="[优先级为5]的订阅方法" />

    <TextView
        android:id="@+id/tv_event_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="[优先级为1]的订阅方法"
        android:layout_marginTop="10dp"/>

    <Button
        android:id="@+id/btn_cancel_delivery"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="点击后优先级为5的拦截消息"
        android:layout_marginTop="20dp"/>
    <Button
        android:id="@+id/btn_turn_next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="跳转到发送事件界面"
        android:layout_marginTop="10dp"/>
    <Button
        android:id="@+id/btn_turn_sticky"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="跳转到粘性事件界面"
        android:layout_marginTop="10dp"/>
    <Button
        android:id="@+id/btn_register_normal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="注册普通事件的监听"
        android:layout_marginTop="5dp"/>
    <Button
        android:id="@+id/btn_unregister_normal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="解除普通事件的注册"/>
    <Button
        android:id="@+id/btn_createcqrode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="生成二维码"/>
    <ImageView
        android:id="@+id/qrcodeImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/ic_launcher"/>
</LinearLayout>
